          "_name": "team2",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"team2 medicalm4_team2\" group=\"Teams\" data-bg-video=\"{{bg.type == 'video'&&bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"formstyler, font-awesome\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"4\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Main Title\" name=\"showMaintitle\" checked>\n        <input type=\"checkbox\" title=\"Name\" name=\"showName\" checked>\n        <input type=\"checkbox\" title=\"Position\" name=\"showPosition\" checked>\n        <input type=\"checkbox\" title=\"Contacts Section\" name=\"showContacts\">\n        <input type=\"checkbox\" title=\"Skills Section\" name=\"showSkills\" checked>\n        <input type=\"checkbox\" title=\"Social Icons Section\" name=\"showSocIcons\">\n        <input type=\"checkbox\" title=\"Capabilities Section\" name=\"showCapabilities\" checked>\n        <input type=\"checkbox\" title=\"Form Section\" name=\"showForm\">\n        <header>Size</header>\n        <select title=\"Progress count\" name=\"progressCount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n        </select>\n        <select title=\"Info count\" name=\"infoCount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\" selected>8</option>\n        </select>\n\n        <input type=\"color\" title=\"Progress Color\" name=\"progressColor1\" value=\"#4f87fb\">\n        <input type=\"color\" title=\"Progress Color\" name=\"progressColor2\" value=\"#4f87fb\" condition=\"progressCount>1\">\n        <input type=\"color\" title=\"Progress Color\" name=\"progressColor3\" value=\"#4f87fb\" condition=\"progressCount>2\">\n        <input type=\"color\" title=\"Progress Color\" name=\"progressColor4\" value=\"#4f87fb\" condition=\"progressCount>3\">\n\n\n        <input type=\"range\" inline title=\"Progress 1\" name=\"progress1\" min=\"0\" max=\"100\" step=\"1\" value=\"100\">\n        <input type=\"range\" inline title=\"Progress 2\" name=\"progress2\" min=\"0\" max=\"100\" step=\"1\" value=\"94\" condition=\"progressCount>1\">\n        <input type=\"range\" inline title=\"Progress 3\" name=\"progress3\" min=\"0\" max=\"100\" step=\"1\" value=\"89\" condition=\"progressCount>2\">\n        <input type=\"range\" inline title=\"Progress 4\" name=\"progress4\" min=\"0\" max=\"100\" step=\"1\" value=\"84\" condition=\"progressCount>3\">\n        <header>Style</header>\n        <input type=\"color\" name=\"socialIconsColor\" title=\"Social Icons Color\" value=\"#4f87fb\">\n        <input type=\"color\" name=\"capabilitiesIcon\" title=\"Capabilities Icon Color\" value=\"#4f87fb\">\n        <input type=\"checkbox\" name=\"customStyle\" title=\"Custom Style\" checked>\n        <input type=\"color\" name=\"fieldColor\" title=\"Field\" value=\"#fafafa\" condition=\"customStyle\">\n        <input type=\"color\" name=\"fieldHover\" title=\"Field Hover\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderColor\" title=\"Border\" value=\"#ececec\" condition=\"customStyle\">\n        <input type=\"color\" name=\"borderHover\" title=\"Border Hover\" value=\"#ffffff\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formText\" title=\"Text\" value=\"#000000\" condition=\"customStyle\">\n        <input type=\"color\" name=\"formPlaceholder\" title=\"Placeholders\" value=\"#6d7a8c\" condition=\"customStyle\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" value=\"@ADDONS_PATH@/medicalm4/components/_images/background1.jpg\" title=\"Image\">\n            <input type=\"color\" value=\"#ffffff\" title=\"Color\" selected>\n            <input type=\"video\" value=\"https://www.youtube.com/watch?v=x9vz-axHndk\" title=\"Video\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Color\" name=\"overlayColor\" value=\"#656565\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay&&bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <div class=\"row row-content justify-content-center\">\n\n            <div class=\"col-lg-4\">\n                <div class=\"team-left-inner\">\n                    <div class=\"featured-wrapper\">\n                        <img src=\"@PROJECT_PATH@/assets/images/fb-pic-1-768x768.jpg\" title alt=\"Ryan_key\">\n                    </div>\n                    <div class=\"team-info-block\">\n                        <h2 class=\"team-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".team-left-inner .team-info-block .team-title\" mbr-if=\"showName\">Ryan Key</h2>\n                        <h4 class=\"team-designation mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".team-left-inner .team-info-block .team-designation\" mbr-if=\"showPosition\">\n                            UX/UI Designer</h4>\n                        <div class=\"short-description\">\n                            <p class=\"mbr-text mbr-fonts-style mbr-normal\" mbr-theme-style=\"display-4\" data-app-selector=\".team-left-inner .team-info-block .short-description p\">\n                                UX designer is concerned with the entire process of acquiring and integrating a product, including aspects of branding, design, usability, and function. It is a story that begins before the device is even in the user's hands. <i>“No product is an island. A product is more than a product.''</i></p>\n                        </div>\n                    </div>\n                    <div class=\"team-info-block\" mbr-if=\"showContacts\">\n                        <h3 class=\"personal-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".team-left-inner .team-info-block .personal-title\">Plastic Surgeon</h3>\n                        <div class=\"mbr-list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-list\" data-multiline mbr-list>\n                            <ul class=\"single-team-info\">\n                                <li mbr-if=\"infoCount > 0\"><label>Phone</label>+6012-7740280</li>\n                                <li mbr-if=\"infoCount > 1\"><label>Email</label>info@ryankey.com</li>\n                                <li mbr-if=\"infoCount > 2\"><label>Website</label>mobirise.com</li>\n                                <li mbr-if=\"infoCount > 3\"><label>Fax</label>987654321</li>\n                                <li mbr-if=\"infoCount > 4\"><label>Fax</label>987654321</li>\n                                <li mbr-if=\"infoCount > 5\"><label>Fax</label>987654321</li>\n                                <li mbr-if=\"infoCount > 6\"><label>Fax</label>987654321</li>\n                                <li mbr-if=\"infoCount > 7\"><label>Fax</label>987654321</li>\n                            </ul>\n                        </div>\n\n                    </div>\n                    <div class=\"team-info-block\" mbr-if=\"showSkills\">\n                        <h3 class=\"personal-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".team-left-inner .team-info-block .personal-title\">Skill</h3>\n                        <div class=\"progress_elements\">\n                            <div class=\"progress1 pb-4\">\n                                <div class=\"title-wrap\">\n                                    <div class=\"progressbar-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\">\n                                        <p data-app-selector=\".progressbar-title p, .progress_value\">\n                                            UX/UI </p>\n                                    </div>\n                                </div>\n                                <progress class=\"progress progress-primary\" value=\"{{progress1}}\" max=\"100\">\n                                </progress>\n                            </div>\n\n                            <div class=\"progress2 pb-4\" mbr-if=\"progressCount > 1\">\n                                <div class=\"title-wrap\">\n                                    <div class=\"progressbar-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\">\n                                        <p data-app-selector=\".progressbar-title p, .progress_value\">\n                                            Web Designs </p>\n                                    </div>\n                                </div>\n                                <progress class=\"progress progress-primary\" value=\"{{progress2}}\" max=\"100\">\n                                </progress>\n                            </div>\n                            <div class=\"progress3 pb-4\" mbr-if=\"progressCount > 2\">\n                                <div class=\"title-wrap\">\n                                    <div class=\"progressbar-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\">\n                                        <p data-app-selector=\".progressbar-title p, .progress_value\">\n                                            Interface</p>\n                                    </div>\n                                </div>\n                                <progress class=\"progress progress-primary\" value=\"{{progress3}}\" max=\"100\">\n                                </progress>\n                            </div>\n\n                            <div class=\"progress4\" mbr-if=\"progressCount > 3\">\n                                <div class=\"title-wrap\">\n                                    <div class=\"progressbar-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\">\n                                        <p data-app-selector=\".progressbar-title p, .progress_value\">\n                                            Design</p>\n                                    </div>\n                                </div>\n                                <progress class=\"progress progress-primary\" value=\"{{progress4}}\" max=\"100\">\n                                </progress>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"team-info-block\" mbr-if=\"showSocIcons\">\n                        <div class=\"social\">\n                            <span mbr-icon class=\"mbr-iconfont socicon socicon-facebook\"></span>\n                            <span mbr-icon class=\"mbr-iconfont socicon socicon-twitter\"></span>\n                            <span mbr-icon class=\"mbr-iconfont socicon socicon-linkedin\"></span>\n                        </div>\n                    </div>\n\n                </div>\n            </div>\n            <div class=\"col-lg-8\">\n                <div class=\"entry-content\">\n                    <div class=\"text-box\">\n                        <h4 class=\"card-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showMaintitle\" data-app-selector=\".entry-content .text-box .card-title\">About me</h4>\n\n                        <p class=\"mbr-text mbr-fonts-style mbr-normal doc-descr\" mbr-theme-style=\"display-4\" data-app-selector=\".entry-content .text-box .doc-descr\">我的名字是Ryan Key, 我现在是Full-Time Freelancer, 主攻网页和手机应用程式(App)设计, 之前在一间香港Agency做Web Designer已经20年, 刚开始是由平面设计, 之后被公司委派去学习UI/UX, 之后负责的工作包括起开发系统, 网页设计和解决技术问题. \n<br>\n<br>除了Website, 我还专长包括Branding &amp; Graphic Designs, Mobile Apps, Advertising &amp; Marketing Strategy, 有兴趣者可连络我, 也希望未来我们有机会可以合作.</p>\n                        <p class=\"mbr-text mbr-fonts-style mbr-normal doc-descr\" mbr-theme-style=\"display-4\" data-app-selector=\".entry-content .text-box .doc-descr\">My name is Ryan Key, and I am now a Full-Time Freelancer. I mainly focus on web and mobile application (App) design. I used to be a web designer in a Hong Kong agency for 20 years. At first I was a graphic designer, and then I was appointed by the company. Go to learn UI/UX, and then be responsible for the development of the system, web design and solving technical problems.<br></p>\n                    </div>\n                    <div class=\"capabilities\" mbr-if=\"showCapabilities\">\n                        <h4 class=\"capabilities-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".entry-content .capabilities .capabilities-title, .entry-content .capabilities .mbr-list .list\">Education</h4>\n                        <div class=\"mbr-list mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".entry-content .capabilities .mbr-list .list li\" data-multiline mbr-article>\n                            <ul class=\"list\"><li>The One Academy | Diploma | 1995 - 1997 | 3 years</li>\n                            </ul>\n                        </div>\n                        <h4 class=\"capabilities-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".entry-content .capabilities .capabilities-title, .entry-content .capabilities .mbr-list .list\"><br>Work experience</h4>\n                        <div class=\"mbr-list mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".entry-content .capabilities .mbr-list .list li\" data-multiline mbr-article>\n                            <ul class=\"list\"><li>DF Printer &amp; Supplies Sdn Bhd | Junior Designer | 2 years</li><li>Hanling Taiwan Packaging Co., Ltd. | Packaging Designer | 3 years</li><li>YCH Printer Sdn Bhd | Senior Designer | 2 years</li>\n                                <li>QR Code Solution Sdn Bhd | UI Designer | 5 Year</li>\n                                <li>&nbsp;B&amp;A Multimedia Solution (HK) Co., Ltd. | UX/UI Designer | 8 Year</li>\n                            </ul>\n                        </div>\n                        <p class=\"mbr-text mbr-fonts-style mbr-normal capabilities-text\" mbr-theme-style=\"display-4\" data-app-selector=\".entry-content .capabilities .capabilities-text\">\n                            In addition to Website, I also specialize in Branding &amp; Graphic Designs, Mobile Apps, Advertising &amp; Marketing Strategy. Those who are interested can contact me, I would be glad to collaborate with you! ;)</p>\n                    </div>\n                    <div class=\"contact\" mbr-if=\"showForm\">\n                        <h4 class=\"contact-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".contact .contact-title\">Contact Me</h4>\n                        <!-- -Formbuilder Form--->\n                        <form action name=\"Form Name\" method=\"POST\" class=\"mbr-form form-with-styler\">\n                            <div class=\"row\">\n                                <div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Thanks for\n                                    filling out\n                                    the form!</div>\n                                <div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\">Oops...!\n                                    some\n                                    problem!</div>\n                            </div>\n                            <div class=\"dragArea row\">\n                                <div class=\"col-lg-6 form-group mb-3\" data-for=\"name\">\n                                    <input type=\"text\" name=\"first-name\" placeholder=\"Your Name *\" data-form-field=\"first-name\" mbr-theme-style=\"display-4\" class=\"form-control\" value>\n                                </div>\n                                <div data-for=\"email\" class=\"col-lg-6 form-group mb-3\" style>\n                                    <input type=\"email\" name=\"email\" placeholder=\"Your Email *\" data-form-field=\"email\" mbr-theme-style=\"display-4\" class=\"form-control\" value>\n                                </div>\n                                <div data-for=\"message\" class=\"col-lg-12 form-group mb-3\">\n                                    <textarea name=\"message\" placeholder=\"Message\" data-form-field=\"message\" mbr-theme-style=\"display-4\" class=\"form-control\" value></textarea>\n                                </div>\n                                <div class=\"col-lg-12 mbr-section-btn\" mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\"><a type=\"submit\" class=\"btn btn-black btn-form\" data-app-placeholder=\"Type Text\">SEND\n                                        MESSAGE</a></div>\n                            </div>\n                        </form>\n                        <!---Formbuilder Form- -->\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
